

<% if notice != nil %>
  <div class="alert alert-danger">
    <%= notice %>
</div>
<% end %>
<div>
<h2><%= "There are currently #{@consensus_genes_count.count} genes linked to cancer."%></h2>
</div>
<br>
<div>

</div>
<div class="container clearfix columns manual-table-header">
  <%= form_tag cancers_path, :method => 'get' do %>
   <p>
    <%= text_field_tag :search, params[:search], class: "label-control" %>
    <%= submit_tag "Search", name: nil%>
   </p>
   <% end %>
</div>
<div class="manual-table columns">
  <table>
    <thead>
     <tr>
     </tr>

    </thead>
    <tbody>
     <% @consensus_cancer_genes.sort.each do |cancer_genes| %>
         <button class="cancer-entry marginalize"><%= link_to "#{cancer_genes.gene_symbol}", cancer_path(cancer_genes)%></button>
       <% end %>
    </tbody>
  </table>
</div>
<div class='filler'>

</div>
