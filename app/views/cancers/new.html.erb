<div class="well col-md-3 col-md-offset-1 csvform">
  <h4> CSV import<h4>
    <% if notice != nil %>
    <div class="alert alert-danger">
        <%= notice %>
    </div>
    <%end%>
  <%= form_tag import_cancers_path, multipart: true do %>
    <%= file_field_tag :file, class: "form-control" %>
    <br>
    <%= submit_tag "Import CSV" %>
  <%end%>
</div>
  <div>
    <%= form_for(@cancer, {action: "acs_cancer_list"}) do |f| %>
    <br>
    <div class="form-group">
      <%= f.label :name, class: "control-label" %>
    </div>


    <div class="form-group">
      <%= f.label :genes, class: "control-label" %>
    </div>
      <%= f.submit "Import ACS text" %>
    <% end %>
  </div>
</div>
