<!DOCTYPE html>
<html>
  <head>
    <%= favicon_link_tag '46LYRs.jpg'%>
    <title>BigBangGenetics</title>
    <%= csrf_meta_tags %>
    <%= stylesheet_link_tag 'application', media: 'all'%>
    <!--  d3 before javascript - use d3 obje-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.js" charset="utf-8"></script>
    <script src="http://d3js.org/d3.v3.min.js" charset="utf-8"></script>
    <%= javascript_include_tag 'application'%>
  </head>
  <div>
</div>

<body>
<div class="navbar columns">
  <div class="container">
    <div class="column column-three ">
      <%= link_to("BBGenetics", root_path) %>
    </div>
    <div class="format-nav">
      <% if current_user %>
        <%= link_to "Sign Out", signout_path %>
      <% else  %>
        <%= render "layouts/nav_login" unless @reg == "reg"  %>
      <% end %>
    </div>
  </div>
</div>

    <div class="container">
      <%= yield %>
    </div>
      <footer>
        <div class="footbar columns">
            <%= link_to("Home", root_path) %> |
            <%= link_to("Users", users_path) %> |
            <%= link_to("Cancers", cancers_path) %>
        </div>
      </footer>
  </body>
</html>
